		  <text><![CDATA[2^<font color='#F26200'><b>2</b></font> = <font color='#46A040'><b>4</b></font>]]></text>
		</component>
	<component id="ID_s1_txt03">
	  <text><![CDATA[Borrowing <font color='#F26200'><b>3</b></font> bits:]]></text>
	</component>  
		<component id="ID_s1_txt03B">
		  <text><![CDATA[2^<font color='#F26200'><b>3</b></font> = <font color='#46A040'><b>8</b></font>]]></text>
		</component>
	<component id="ID_s1_txt04">
