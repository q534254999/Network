		<component id="ID_s1_txt05B">
		  <text><![CDATA[2^<font color='#F26200'><b>5</b></font> = <font color='#46A040'><b>32</b></font>]]></text>
		</component>
	<component id="ID_s1_txt06">
	  <text><![CDATA[Borrowing <font color='#F26200'><b>6</b></font> bits:]]></text>
	</component>  
		<component id="ID_s1_txt06B">
		  <text><![CDATA[2^<font color='#F26200'><b>6</b></font> = <font color='#46A040'><b>64</b></font>]]></text>
