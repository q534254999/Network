	</component>  
		<component id="ID_s1_txt08B">
		  <text><![CDATA[2^<font color='#F26200'><b>8</b></font> = <font color='#46A040'><b>256</b></font>]]></text>
		</component>
	<component id="ID_s1_txt09">
	  <text><![CDATA[Borrowing <font color='#F26200'><b>9</b></font> bits:]]></text>
	</component>  
		<component id="ID_s1_txt09B">
		  <text><![CDATA[2^<font color='#F26200'><b>9</b></font> = <font color='#46A040'><b>512</b></font>]]></text>
		</component>
	<component id="ID_s1_txt10">
	  <text><![CDATA[Borrowing <font color='#F26200'><b>10</b></font> bits:]]></text>
	</component>  
		<component id="ID_s1_txt10B">
		  <text><![CDATA[2^<font color='#F26200'><b>10</b></font> = <font color='#46A040'><b>1024</b></font>]]></text>
		</component>
	
	
	
