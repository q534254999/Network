    <component id='ID_s2_txt04'>
        <text>
            <![CDATA[<span style="margin-left:5px;">第一个<b>完整的 VLSM</b> 子网范围</span>]]></text>
    </component>
    <component id='ID_s2_txt05'>
        <text>
            <![CDATA[<span style="margin-left:5px;">最后一个<b>完整的 VLSM</b> 子网范围</span>]]></text>
    </component>
