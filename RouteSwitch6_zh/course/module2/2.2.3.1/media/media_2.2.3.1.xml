    <component id="ID_s3_SC1" fontSize="12" fontFace="_typewriter" fontColor="#ffffff">
        <feedback id="fb"><![CDATA[<b>***</b><font face="_sans" color="#FFFFFF">您必须输入准确而完整的命令。</font><b>***</b> ]]></feedback>
        <existingcommands><![CDATA[<font face="_sans"><b>启用 IPv6 单播路由。</b></font>]]></existingcommands>
       
        <command prompt="R2(config)#">
            <correctresponse><![CDATA[ipv6 unicast-routing]]></correctresponse>
            <instruction><![CDATA[<font face="_sans" class="syntaxBottomLine"><b>您已在 R2 上成功启用 IPv6 单播路由。</b></font>]]></instruction>
